{
    "name": "awnali/laravel-sso-server",
    "description": "Laravel Single Sign-On (SSO) Server Package - Transform any Laravel application into an SSO authentication server",
    "keywords": [
        "laravel",
        "sso",
        "single-sign-on",
        "authentication",
        "server",
        "oauth",
        "session-management",
        "identity-provider",
        "enterprise",
        "security"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Awan Ali",
            "email": "awnali@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "jasny/sso": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Awnali\\LaravelSsoServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awnali\\LaravelSsoServer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awnali\\LaravelSsoServer\\SsoServerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}